<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#fff"
    tools:context=".Fragments.Filtertion">



    <com.crystal.crystalrangeseekbar.widgets.BubbleThumbRangeSeekbar
        android:id="@+id/rangeSeekbar5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:corner_radius="10"
        app:min_value="0"
        app:max_value="20000"
        app:steps="5"
        android:layout_marginEnd="@dimen/_20sdp"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_above="@+id/textMin1"
        app:bar_color="#F7BB88"
        android:layout_marginBottom="@dimen/_10sdp"
        app:bar_highlight_color="#E07416"
        app:left_thumb_image="@drawable/thumb"
        app:right_thumb_image="@drawable/thumb"
        app:left_thumb_image_pressed="@drawable/thumb_pressed"
        app:right_thumb_image_pressed="@drawable/thumb_pressed"
        app:data_type="_integer"/>

    <TextView
        android:id="@+id/textMin1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/_40sdp"
        android:layout_marginTop="5dp"
        android:layout_marginStart="@dimen/_30sdp"
        android:textSize="16dp"
        android:textColor="#000"/>

    <TextView
        android:id="@+id/textMax1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/_40sdp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="@dimen/_30sdp"
        android:textSize="16dp"
        android:textColor="#000"
        android:layout_alignParentRight="true"/>

</RelativeLayout>